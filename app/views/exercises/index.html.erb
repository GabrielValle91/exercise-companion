<p style="color: green"><%= notice %></p>

<h1>Exercises</h1>

<div id="exercises">
  <% @exercises.each do |exercise| %>
    <%= render exercise %>
    <p>
      <%= link_to "Show this exercise", exercise %>
    </p>
  <% end %>
</div>

<%= button_to "New exercise", new_exercise_path, method: :get, data: { turbo_frame: "remote_modal"}, class:"btn btn-primary" %>
